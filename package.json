{
  "name": "dbc-react-autocomplete",
  "version": "0.0.1",
  "description": "DBC autocomplete React components. Giving autocomplete features on a given text input using React.",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run=true",
    "test:watch": "karma start",
    "lint": "esw -w ./src",
    "lint:checkstyle": "eslint ./src -o report.xml -f checkstyle",
    "watch:babel": "babel ./src --ignore=./src/testlib --watch -d dist",
    "watch:bundle": "webpack --progress --colors --watch -d",
    "dev": "parallelshell \"npm run watch:babel\" \"npm run watch:bundle\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCDK/dbc-react-autocomplete"
  },
  "bugs": {
    "url": "/issues"
  },
  "keywords": [
    "react",
    "autocomplete",
    "dbc"
  ],
  "author": "Hr MÃ¸ller",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.3",
    "chai": "^2.3.0",
    "css-loader": "^0.14.3",
    "eslint": "^0.21.2",
    "eslint-loader": "^0.11.2",
    "eslint-plugin-react": "^2.3.0",
    "eslint-watch": "^1.1.1",
    "extract-text-webpack-plugin": "^0.8.1",
    "karma": "^0.12.35",
    "karma-cli": "0.0.4",
    "karma-junit-reporter": "^0.2.2",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.1.2",
    "phantomjs": "^1.9.17",
    "sass-loader": "^1.0.2",
    "wallaby-webpack": "0.0.5",
    "webpack": "^1.9.10"
  },
  "dependencies": {
    "lodash": "^3.9.3",
    "react": "^0.13.3"
  }
}

