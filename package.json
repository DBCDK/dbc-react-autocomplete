{
  "name": "dbc-react-autocomplete",
  "version": "0.0.1",
  "description": "DBC autocomplete React components. Giving autocomplete features on a given text input using React.",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --ignore=./src/testlib -d dist",
    "test": "karma start --single-run=true",
    "test:watch": "karma start",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src -o report.xml -f checkstyle",
    "watch:babel": "babel ./src --ignore=./src/testlib --watch -d dist",
    "watch:bundle": "webpack --progress --colors --watch -d",
    "dev": "parallelshell \"npm run watch:babel\" \"npm run watch:bundle\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCDK/dbc-react-autocomplete"
  },
  "bugs": {
    "url": "https://github.com/DBCDK/dbc-react-autocomplete/issues"
  },
  "keywords": [
    "react",
    "autocomplete",
    "dbc"
  ],
  "author": "Hr MÃ¸ller",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-core": "^5.8.20",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "css-loader": "^0.15.6",
    "eslint": "^1.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.0",
    "eslint-watch": "^1.1.1",
    "extract-text-webpack-plugin": "^0.8.1",
    "karma": "^0.13.8",
    "karma-cli": "0.1.0",
    "karma-junit-reporter": "^0.3.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.1.2",
    "phantomjs": "^1.9.17",
    "sass-loader": "^2.0.0",
    "wallaby-webpack": "0.0.5",
    "webpack": "^1.10.5"
  },
  "dependencies": {
    "lodash": "^3.9.3",
    "react": "^0.13.3"
  }
}
